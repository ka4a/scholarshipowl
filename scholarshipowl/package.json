{
  "name": "scholarshipowl",
  "version": "1.0.0",
  "description": "A system to streamline the process of scholarship application for US students. Built on Laravel 4, with bootstrap for front-end.",
  "main": "index.js",
  "engines": {
    "node": ">=6",
    "npm": ">=3.8"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "assets:all": "node -e 'require(\"./tasks.js\").assetsAll()'",
    "assets:js": "node -e 'require(\"./tasks.js\").assetsJs()'",
    "assets:css": "node -e 'require(\"./tasks.js\").assetsCss()'",
    "test": "mocha-webpack --watch --webpack-config webpack.test.js --require resources/assets/js/test/setup.js resources/assets/js/test/unit/{**/,**/**/}*.spec.js",
    "watch": "./node_modules/.bin/gulp icon-font && ./node_modules/.bin/gulp sass && ./node_modules/.bin/gulp clear:build-dir && ./node_modules/.bin/webpack-dev-server --config webpack.dev.js",
    "build": "./node_modules/.bin/gulp clear:build-dir && ./node_modules/.bin/webpack --config webpack.prod.js",
    "browser-sync": "./node_modules/.bin/gulp browser-sync"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/scholarshipowl/scholarshipowl.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/scholarshipowl/scholarshipowl/issues"
  },
  "homepage": "https://gitlab.com/scholarshipowl/scholarshipowl#README",
